######## Use Item ########

useSelf_item Battle Manual {
	whenStatusInactive Field Manual, EFST_CASH_PLUSEXP
	inLockOnly 1
	onAction attack
	timeout 1805
}

useSelf_item Bubble Gum {
	whenStatusInactive Bubble Gum, EFST_CASH_RECEIVEITEM
	inLockOnly 1
	onAction attack
	timeout 1805
}

useSelf_item [Event] Battle Manual {
	whenStatusInactive Field Manual, EFST_CASH_PLUSEXP
	inInventory Battle Manual < 1
	inLockOnly 1
	onAction attack
	timeout 1805
}

useSelf_item [Event] Bubble Gum {
	whenStatusInactive Bubble Gum, EFST_CASH_RECEIVEITEM
	inInventory Bubble Gum < 1
	inLockOnly 1
	onAction attack
	timeout 1805
}

useSelf_item [Event] Battle Bubble {
	whenStatusInactive Field Manual, Bubble Gum, EFST_CASH_PLUSEXP, EFST_CASH_RECEIVEITEM
	inInventory [Event] Battle Manual < 1
	inLockOnly 1
	onAction attack
	timeout 1805
}

